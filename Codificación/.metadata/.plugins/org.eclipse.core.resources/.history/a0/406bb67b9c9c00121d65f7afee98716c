package datos;

import java.util.ArrayList;
import java.util.Collection;

public class Producto 
{
	//ATRIBUTOS
	int idProducto;
	String nombre;
	int existenciaStock;
	private Collection<datos.Precio> precios;
	
	
	//CONSTRUCTOR
	public Producto() 
	{
		this.idProducto = 0;
		this.nombre = "";
		this.existenciaStock = 0;
		this.precios = new ArrayList<datos.Precio>();
	}
}
